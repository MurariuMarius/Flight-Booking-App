<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.fis.project.flightbookingapp.controllers.BookingRequestsManagementController">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0">
         <center>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
                  <TableView fx:id="tableView" prefHeight="246.0" prefWidth="441.0">
                    <columns>
                      <TableColumn fx:id="bookingId" prefWidth="96.0" text="Booking No." />
                      <TableColumn fx:id="clientUsername" prefWidth="54.39990234375" text="Client" />
                        <TableColumn fx:id="flightNumber" prefWidth="103.2000732421875" text="Flight Number" />
                        <TableColumn fx:id="date" prefWidth="75.0" text="Date" />
                        <TableColumn fx:id="travellers" prefWidth="75.0" text="Travellers" />
                        <TableColumn fx:id="actionColumn" prefWidth="75.0" text="Action" />
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                     <opaqueInsets>
                        <Insets left="50.0" right="50.0" />
                     </opaqueInsets>
                  </TableView>
               </children>
               <opaqueInsets>
                  <Insets left="50.0" right="50.0" />
               </opaqueInsets>
            </VBox>
         </center>
         <top>
            <Button fx:id="homeButton" mnemonicParsing="false" text="Home" BorderPane.alignment="CENTER" />
         </top>
      </BorderPane>
   </children>
</AnchorPane>
